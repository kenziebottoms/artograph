    "nodemon": "^1.17.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "request": "^2.85.0",
    "sequelize": "^4.37.6"
  },
  "devDependencies": {
    "mocha": "^5.1.1"
  },
  "scripts": {
    "test": "mocha -R nyan --watch",
    "start": "nodemon server/server.js",
    "db:regen": "node server/db/createDb.js",
    "css:build": "node-sass client/sass/style.sass client/css/style.css",
    "css:watch": "nodemon -e sass -x \"npm run css:build\"",
    "db:save": "pg_dump artograph > server/db/export.sql",
    "db:reset": "npm run db:wipe && psql artograph < server/db/export.sql",
    "db:wipe": "psql -c 'drop database if exists artograph;' && psql -c 'create database artograph;'"
  },
